<p id="notice"><%= notice %></p>
<div class="container">
<div class="row">
  <div class="col-lg-12">
<div class="card">
  <div class="card-header">
   Lista de propuestas
 </div>
  <div class="card-body">
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Nombre</th>
      <th>titulo</th>
      <th>Correo</th>
      <th>organizacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @propuestas.each do |propuestum| %>
      <tr>
        <td><%= propuestum.nombre %></td>
        <td><%= propuestum.titulo %></td>
        <td><%= propuestum.correo %></td>
        <td><%= propuestum.organizacion %></td>
        <td><%= link_to 'Editar datos', edit_user_propuesta_path(propuestum), class:"btn btn-default btn-primary"  %></td>
        <td><%= link_to 'Ver', user_propuesta_path(propuestum), class:"btn btn-default btn-primary"  %></td>

      </tr>
    <% end %>
  </tbody>
</table>
  </div>
  <div class="card-footer text-muted">
    <%= link_to 'Crear nueva asistencia', new_user_propuesta_path , class:"btn btn-default btn-primary"%>
    <%= link_to 'Volver', root_path, class:"btn btn-default btn-primary" %>
  </div>
</div>
</div>
</div>
</div>
<br>
