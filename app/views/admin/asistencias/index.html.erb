<p id="notice"><%= notice %></p>
<div class="container">
<div class="row">
  <div class="col-lg-12">
<div class="card">
  <div class="card-header">
   Lista de asistencia
 </div>
  <div class="card-body">
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Correo</th>
      <th>Teléfono</th>
      <th>Institución</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asistencias.each do |asistencium| %>
      <tr>
        <td><%= asistencium.nombre %></td>
        <td><%= asistencium.apellido %></td>
        <td><%= asistencium.correo %></td>
        <td><%= asistencium.telefono %></td>
        <td><%= asistencium.institucion %></td>
        <td><%= link_to 'Editar datos', edit_admin_asistencia_path(asistencium), class:"btn btn-default btn-primary"  %></td>

      </tr>
    <% end %>
  </tbody>
</table>
  </div>
  <div class="card-footer text-muted">
    <%= link_to 'Crear nueva asistencia', new_admin_asistencia_path , class:"btn btn-default btn-primary"%>
    <%= link_to 'Volver', root_path, class:"btn btn-default btn-primary" %>
  </div>
</div>
</div>
</div>
</div>
<br>
